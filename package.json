{
  "name": "api-test",
  "version": "1.0.0",
  "description": "cypress automation api",
  "main": "index.js",
  "devDependencies": {
    "cypress": "^8.0.0",
    "cypress-cucumber-preprocessor": "^4.1.4",
    "faker": "^5.5.3",
    "multiple-cucumber-html-reporter": "^1.18.0"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": true,
    "nonGlobalStepBaseDir": "cypress/support/steps"
  },
  "scripts": {
    "cypress": "npx cypress run",
    "cypress:open": "npx cypress open",
    "report": "node cypress/support/utils/htmlReporter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucassev05/api-test.git"
  },
  "keywords": [
    "api",
    "test",
    "cypress"
  ],
  "author": "Lucas Evangelista",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lucassev05/api-test/issues"
  },
  "homepage": "https://github.com/Lucassev05/api-test#readme"
}
